{
  "scripts": {
    "lh": "lighthousebot",
    "pwmetrics": "./node_modules/pwmetrics/bin/cli.js --config=package.json"
  },
  "dependencies": {
    "pwmetrics": "^4.2.3"
  },
  "devDependencies": {
    "minimist": ">=1.2.2",
    "dot-prop": ">=4.2.1",
    "yargs-parser": ">=13.1.2",
    "node-forge": ">=0.10.0",
    "xmldom": ">=0.5.0"
  },
  "pwmetrics": {
    "url": "http://iasc8.local.docksal/needs-assessment-task-force-meeting-0",
    "flags": {
      "logLevel": "debug",
      "outputPath": "stdout",
      "runs": 3,
      "expectations": true,
      "chromeFlags": "--headless",
      "failOnError": true,
      "showOutput": true
    },
    "expectations": {
      "firstMeaningfulPaint": {
        "warn": ">=3000",
        "error": ">=6000"
      },
      "firstContentfulPaint": {
        "warn": ">=3000",
        "error": ">=6000"
      },
      "firstCPUIdle": {
        "warn": ">=3000",
        "error": ">=6000"
      },
      "interactive": {
        "warn": ">=4000",
        "error": ">=8000"
      },
      "speedIndex": {
        "warn": ">=3000",
        "error": ">=6000"
      }
    }
  }
}
